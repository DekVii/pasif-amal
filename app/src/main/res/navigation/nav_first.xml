<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_first"
    app:startDestination="@layout/fragment_login"
    tools:ignore="UnusedNavigation">


    <fragment
        android:id="@+id/Login"
        android:name="com.example.pasifamal.ui.LoginFragment"
        android:label="Fragment One"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_fLogin_to_fRegister"
            app:destination="@+id/Register"/>
        <action
            android:id="@+id/action_fLogin_to_fHome"
            />
    </fragment>
    <fragment
        android:id="@+id/Register"
        android:name="com.example.pasifamal.ui.RegisterFragment"
        android:label="Fragment Two"
        tools:layout="@layout/fragment_register">
        <action
            android:id="@+id/action_fRegister_to_fLogin"
            app:destination="@+id/Login"/>
    </fragment>

</navigation>